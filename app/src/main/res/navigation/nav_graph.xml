<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.mandiri.goldmarket.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_pocketFragment"
            app:destination="@id/pocketFragment" />
    </fragment>
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.mandiri.goldmarket.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.mandiri.goldmarket.ProfileFragment"
        android:label="ProfileFragment" />
    <fragment
        android:id="@+id/pocketFragment"
        android:name="com.mandiri.goldmarket.PocketFragment"
        android:label="fragment_pocket"
        tools:layout="@layout/fragment_pocket" />
</navigation>